#! /bin/sh
#BSUB -q %QUEUE%
#BSUB -P %PROJ:SPACE%-%PROJENVIR%
#BSUB -J %E%wdas_analysis_high_noda_%CYC%
#BSUB -o %COM%/output/%ENVIR%/today/wdas_analysis_high_noda_%CYC%.o%J
#BSUB -W 00:05
#BSUB -n 1
#BSUB -R affinity[core]
#BSUB -R span[ptile=1]
#BSUB -M 2000
#BSUB -cwd /tmp

%include <head.h>
%include <envir-p3.h>

set -x

export NET=%NET:wfs%
export RUN=%RUN%
export CDUMP=%RUN%

model=wamipe
%include <model_ver.h>

############################################################
# Load modules
############################################################
module load EnvVars/$EnvVars_ver
module load lsf/$lsf_ver
module load ips/$ips_ver
module load impi/$impi_ver
module load crtm/$crtm_ver
module load CFP/$CFP_ver
module load NetCDF/$NetCDF_ver
module load HDF5-parallel/$HDF5_parallel_ver
module load grib_util/$grib_util_ver

module list

#%include <ecf_client.h>
export USE_CFP=YES
export ROTDIR=%COM%
export cyc=%CYC%
export cycle=t%CYC%z
export PREINP=${CDUMP}.${cycle}.

############################################################
# CALL executable job script here
############################################################
${HOMEwamipe}/jobs/JWAMIPE_ANALYSIS

if [ $? -ne 0 ]; then
   ecflow_client --msg="***JOB ${ECF_NAME} ERROR RUNNING J-SCRIPT ***"
   ecflow_client --abort
   exit
fi

%include <tail.h>
%manual

%end
